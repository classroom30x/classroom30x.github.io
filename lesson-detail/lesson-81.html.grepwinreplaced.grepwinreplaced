			<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

			
 <script>
        $(document).ready(function() {
             let games = [];

             $.getJSON('https://classroom30x.github.io/lessons.json', function(data) {
                games = data;
            });

             $('#search-input').on('input', function() {
                const input = $(this).val().toLowerCase();
                const resultsContainer = $('#search-results');
                resultsContainer.empty(); // Önceki sonuçları temizle

                if (input.length >= 2) {
					$(".search-results").show();
                    const filteredGames = games.filter(game => game.url.toLowerCase().includes(input)).slice(0, 5);
                    
                    filteredGames.forEach(game => {
                        const gameElement = $('<div class="sikik_selo_liste"></div>');
                        const gameImage = $('<img width="40">').attr('src', `https://classroom30x.github.io/lessons-img/${game.lesson.toLowerCase()}.webp`).attr('alt', game.url);
                        const gameLink = $('<a></a>').attr('href','https://classroom30x.github.io/lesson-detail/'+ game.lesson+'.html').text(game.url);
                        gameElement.append(gameImage);
                        gameElement.append(gameLink);
                        resultsContainer.append(gameElement);
                    });
                } else {
					$(".search-results").hide();
				}
            });
        });
    </script>   		<br>
